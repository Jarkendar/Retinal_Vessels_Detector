<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="266.0" prefWidth="428.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <children>
        <Button fx:id="imageButton" layoutX="14.0" layoutY="14.0" minWidth="400.0" mnemonicParsing="false" text="Choose File to learn" onAction="#chooseImage"/>
        <Button fx:id="expertButton" layoutX="14.0" layoutY="50.0" minWidth="400.0" mnemonicParsing="false" text="Choose expert mask" onAction="#chooseExpert"/>
        <Button fx:id="areaButton" layoutX="14.0" layoutY="86.0" minWidth="400.0" mnemonicParsing="false" text="Choose image area" onAction="#chooseArea"/>
        <Button fx:id="arffButton" layoutX="14.0" layoutY="122.0" minWidth="400.0" mnemonicParsing="false" text="Choose arff file" onAction="#chooseARFF"/>
        <Label fx:id="statusLabel" layoutX="14.0" layoutY="158.0" minWidth="400.0" alignment="CENTER" text="Status: Empty"/>
        <Button fx:id="dataSetButton" layoutX="14.0" layoutY="194.0" minWidth="400.0" mnemonicParsing="false" text="Create data set arff" disable="true" onAction="#startPreparingOfDataSet"/>
        <Button fx:id="modelButton" layoutX="14.0" layoutY="230.0" minWidth="400.0" mnemonicParsing="false" text="Create model" disable="true" onAction="#startLearnModel"/>
    </children>
</AnchorPane>
